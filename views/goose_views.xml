    <record id="view_goose_order_filter" model="ir.ui.view">
        <field name="name">goose.order.search</field>
        <field name="model">goose.order</field>
        <field name="arch" type="xml">
            <search>
                <field name="client_bh" string="客戶编号" filter_domain="[('client_bh','ilike',self)]"/>
                <field name="client_id" string="客戶名称" filter_domain="[('client_id','ilike',self)]"/>
                <field name="liushui"/>
                <field name="madan"/>
                <filter string="计量" name="bujiliang" domain="[('bujiliang','=',False)]"/>
                <group expand="0" string="Group By">
                    <filter string="客户" domain="[('bujiliang','=','False)]" context="{'group_by':'client_id'}"/>
                    <filter string="码单" name="order_date" context="{'group_by':'madan'}"/>
                </group>
            </search>
        </field>
    </record>

