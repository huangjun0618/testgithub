<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <record id="seq_sale_price_contract" model="ir.sequence">
            <field name="name">购销合同</field>
            <field name="code">sale.price.contract</field>
            <field name="prefix">H%(year)s</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_goose_order" model="ir.sequence">
            <field name="name">订单</field>
            <field name="code">goose.order</field>
            <field name="prefix">D%(year)s</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_goose_receipt" model="ir.sequence">
            <field name="name">收款</field>
            <field name="code">goose.receipt</field>
            <field name="prefix">S%(year)s</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
