<?xml version="1.0" encoding="utf-8" ?>

<odoo>
    <!--客户基本档-->

    <record id="goose_client_form_view" model="ir.ui.view">
        <field name="name">goose client form</field>
        <field name="model">goose.client</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group colspan="2" string="客户基本档">
                        <group>
                            <field name="code"/>
                            <field name="jiancheng"/>
                            <field name="name"/>
                            <field name="name_en"/>
                            <field name="registered_capital"/>
                            <field name="registered_date"/>
                            <field name="registered_address"/>
                            <field name="hand_phone"/>
                            <field name="phone"/>
                            <field name="fax"/>
                            <field name="company_tax_number"/>
                            <field name="payment_method"/>
                            <field name="is_partner"/>
                            <field name="user_id"/>
                        </group>
                        <group>
                            <field name="faren"/>
                            <field name="new_or_not"/>
                            <field name="yewu"/>
                            <field name="yewu_code"/>
                            <field name="business_license"/>
                            <field name="business_mode"/>
                            <field name="zip_code"/>
                            <field name="company_website"/>
                            <field name="e_mail"/>
                            <field name="company_account"/>
                            <field name="bank_account"/>
                            <field name="credit_quota"/>
                        </group>
                        <group colspan="2" string="联系人">
                            <field name="contacts" mode="tree,kanban">
                                <tree editable="bottom">
                                    <field name="name"/>
                                    <field name="duties"/>
                                    <field name="department"/>
                                    <field name="hand_phone"/>
                                    <field name="phone"/>
                                    <field name="other_way"/>
                                    <field name="remark"/>
                                </tree>
                            </field>
                        </group>
                        <group colspan="2" string="送货地址">
                            <field name="delivery_address"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="goose_client_tree_view" model="ir.ui.view">
        <field name="name">goose client tree view</field>
        <field name="model">goose.client</field>
        <field name="arch" type="xml">
            <tree string="客户信息">
                <field name="code"/>
                <field name="name"/>
                <field name="yewu_code"/>
                <field name="yewu"/>
                <field name="contacts"/>
            </tree>
        </field>
    </record>

    <record id="goose_client_action" model="ir.actions.act_window">
        <field name="name">客户信息</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">goose.client</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
        <field name="context">{}</field>
    </record>

